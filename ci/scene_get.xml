<Configuration>
  <Behavior>
    <Multi_Message>True</Multi_Message>
    <Include_CSV>True</Include_CSV>
    <Parse_Responses>True</Parse_Responses>
    <Fail_On_Response>True</Fail_On_Response>
  </Behavior>
  <Message>
    <Message_Location>ci/json/scene_small.json</Message_Location>
    <Message_Extension>json</Message_Extension>
    <CSV_Location>ci/csv/scene_get.csv</CSV_Location>
    <Variable_Start_Character>$</Variable_Start_Character>
    <Variable_End_Character>@</Variable_End_Character>
  </Message>
  <ZeroMQ>
    <Outbound_Connection>tcp://localhost:5555</Outbound_Connection>
    <Outbound_Connection_Type>REQ</Outbound_Connection_Type>
    <Timeout>5000</Timeout>
  </ZeroMQ>
  <Logging>
    <Log_File>scene_get.log</Log_File>
    <Log_Level>Info</Log_Level>
  </Logging>
  <Response>
    <Field_Path>err_code</Field_Path>
    <Success_Value>100</Success_Value>
  </Response>
</Configuration>
